<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4" main_tree_to_execute="SimpleDepthMissionOnly">
    <BehaviorTree ID="SimpleDepthMissionOnly">
        <Sequence name="depth_mission_sequence">
            <Wait name="Getting_Ready" duration="5"/>
            <LaunchSimpleDepthMission name="simple_depth_mission"/>
            <Wait name="mission_completed" duration="2"/>
        </Sequence>
    </BehaviorTree>
    
    <!-- Description of Node Models (used by Groot) -->
    <TreeNodesModel>
        <Action ID="LaunchSimpleDepthMission"/>
        <Action ID="Wait">
            <input_port name="duration" type="Int">Wait duration in seconds</input_port>
        </Action>
    </TreeNodesModel>
</root>